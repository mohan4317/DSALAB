#include <stdio.h>
#include <stdlib.h>
void TOH (int n,char d, char t, char s)
{
    if(n>1)
    {
        TOH((n-1),s,t,d);
        printf("\n Move %d disc from %c to %c",n,s,d);
        TOH((n-1),t,d,s);
    }
    else
        printf("\n Move %d disc from %c to %c",n,s,d);

}
int main()

{
    int n;
    printf("\n Read number of discs:");
    scanf("%d",&n);
    TOH(n,'S','D','T');
    return 0;
}
